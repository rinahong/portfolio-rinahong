<%- include('../partials/header') %>
  <header class="intro-bg" id="intro-content">
    <div class="container">
      <div class="intro-text">
        <% if (messages) { %>
          <div class="alert alert-<%=status%> alert-dismissible">
            <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
            <%= messages %>
          </div>
        <% } %>
        <p id="header-text"><strong>SOFTWARE / WEB DEVELOPER</strong></p>
        <a href="https://github.com/rinahong" target="_blank" style="color:white">
          <span class="link-url"><i>github.com/rinahong</i></span>
        </a> |
        <a href="https://www.linkedin.com/in/rinahong/" target="_blank" style="color:white">
          <span class="link-url"><i>linkedin.com/in/rinahong</i></span>
        </a>
        <form method="get" action="../images/RinaHong_Resume.pdf" target="_blank">
          <button type="submit" id="resume-download">
            <h5 id="resume-download-text"><strong>DOWNLOAD RESUME</strong></h5>
          </button>
        </form>
      </div>
    </div>
  </header>

  <section class="contents" id="services">
    <div class="container">
      <div class="row space-top">
        <div class="col-lg-12 text-center">
          <p class="section-header-text"><strong>SERVICES</strong></p>
          <h3 class="text-muted">Technical skills I can provide</h3>
        </div>
      </div>
      <div class="row text-center space-top">
        <div class="col-md-4">
          <img class="services-image img-fluid" src="../images/front-end.png" alt="frontend">
          <h4 class="service-heading">Front-End</h4>
          <p class="text-muted">HTML5, CSS, JavaScript, jQuery, Ajax, Bootstrap, React.js, React Native, JSF, SJR</p>
        </div>
        <div class="col-md-4">
          <img class="services-image img-fluid" src="../images/back-end.png" alt="backend">
          <h4 class="service-heading">Back-End</h4>
          <p class="text-muted">Ruby on Rails, Node.js, Express.js, Java, ASP.NET MVC and CORE, C++, C, MySQL, PostgreSQL, ActiveRecord</p>
        </div>
        <div class="col-md-4">
          <img class="services-image img-fluid" src="../images/other.png" alt="other">
          <h4 class="service-heading">Others</h4>
          <p class="text-muted">Heroku, Git, GitHub, AWS</p>
        </div>
      </div>
    </div>
  </section>


